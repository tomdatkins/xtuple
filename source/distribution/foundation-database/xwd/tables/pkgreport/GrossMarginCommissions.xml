  <name>head</name>
  <sql>SELECT &lt;? if exists("outsiderep_id") ?>
         ( SELECT salesrep_name
             FROM salesrep
            WHERE (salesrep_id=&lt;? value("outsiderep_id") ?>) )
       &lt;? else ?>
         text('All Sales Reps')
       &lt;? endif ?>
       AS outsiderep,
       &lt;? if exists("insiderep_id") ?>
         ( SELECT salesrep_name
             FROM salesrep
            WHERE (salesrep_id=&lt;? value("insiderep_id") ?>) )
       &lt;? else ?>
         text('All Sales Reps')
       &lt;? endif ?>
       AS insiderep,
       formatDate(&lt;? value("startDate") ?>) AS startdate,
       formatDate(&lt;? value("endDate") ?>) AS enddate;</sql>
