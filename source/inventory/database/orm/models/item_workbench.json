        "name": "orderMaximum",
        "attr": {
          "type": "Quantity",
          "derived": true,
          "method": "itemsite_maxordqty"
        }
      },
      {
        "name": "orderTo",
        "attr": {
          "type": "Quantity",
          "derived": true,
          "method": "itemsite_ordertoqty"
        }
