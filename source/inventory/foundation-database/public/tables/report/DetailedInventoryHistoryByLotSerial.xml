 <querysource>
  <name>detail</name>
  <sql>SELECT (REPEAT(' ',(lshist_level-1)*4) || lshist_level) AS level,
       lshist_warehous_code AS warehous_code,
       formatDateTime(lshist_transdate) AS transdate,
       lshist_transtype AS transtype,
       lshist_ordernumber AS ordernumber,
       invhist_invuom,
       item_number,
       item_descrip1,
       item_descrip2,
       lshist_lotserial as lotserial,
       lshist_locationname AS location_name,
       formatDate(lshist_expiration) AS expiration,
       formatQty(lshist_transqty) AS transqty,
       formatQty(lshist_qty_before) AS qohbefore,
       formatQty(lshist_qty_after) AS qohafter,
       invhist_user AS username,

-- HMRF add SO info (4.9.3)
     CASE WHEN (lshist_transtype = 'SH' AND 
                SUBSTRING(lshist_ordernumber FROM 1 FOR 2) = 'SO')
          THEN (SELECT cust_number || '-' || cust_name FROM custinfo 
                       JOIN cohead ON (cust_id = cohead_cust_id)
                       WHERE cohead_number = TRIM(SUBSTRING(lshist_ordernumber FROM '-.*-'), '-')
               ) ELSE '' END AS customerinfo,
              
     CASE WHEN (lshist_transtype = 'SH' AND 
                SUBSTRING(lshist_ordernumber FROM 1 FOR 2) = 'SO')
          THEN (SELECT cohead_shiptoname FROM cohead
                       WHERE cohead_number = TRIM(SUBSTRING(lshist_ordernumber FROM '-.*-'), '-')
               ) ELSE '' END AS shiptoname,   

     CASE WHEN (lshist_transtype = 'SH' AND 
                SUBSTRING(lshist_ordernumber FROM 1 FOR 2) = 'SO')
          THEN (SELECT cohead_shiptoaddress1 FROM cohead
                      WHERE cohead_number = TRIM(SUBSTRING(lshist_ordernumber FROM '-.*-'), '-')
               ) ELSE '' END AS shiptol1,

    CASE WHEN (lshist_transtype = 'SH' AND 
                SUBSTRING(lshist_ordernumber FROM 1 FOR 2) = 'SO')
          THEN (SELECT cohead_shiptoaddress2 FROM cohead
                        WHERE cohead_number = TRIM(SUBSTRING(lshist_ordernumber FROM '-.*-'), '-')
               ) ELSE '' END AS shiptol2,  

     CASE WHEN (lshist_transtype = 'SH' AND 
                SUBSTRING(lshist_ordernumber FROM 1 FOR 2) = 'SO')
          THEN (SELECT cohead_shiptoaddress3 FROM cohead
                       WHERE cohead_number = TRIM(SUBSTRING(lshist_ordernumber FROM '-.*-'), '-')
               ) ELSE '' END AS shiptol3,   

    CASE WHEN (lshist_transtype = 'SH' AND 
                SUBSTRING(lshist_ordernumber FROM 1 FOR 2) = 'SO')
          THEN (SELECT cohead_shiptocity || ', ' || cohead_shiptostate || ' ' || cohead_shiptozipcode FROM cohead
                       WHERE cohead_number = TRIM(SUBSTRING(lshist_ordernumber FROM '-.*-'), '-')
               ) ELSE '' END AS citystatezip,    

    CASE WHEN (lshist_transtype = 'SH' AND 
                SUBSTRING(lshist_ordernumber FROM 1 FOR 2) = 'SO')
          THEN (SELECT cohead_shiptocountry FROM cohead
                       WHERE cohead_number = TRIM(SUBSTRING(lshist_ordernumber FROM '-.*-'), '-')
               ) ELSE '' END AS country,    

    CASE WHEN (lshist_transtype = 'SH' AND 
                SUBSTRING(lshist_ordernumber FROM 1 FOR 2) = 'SO')
          THEN (SELECT cohead_shipto_cntct_honorific || ' ' || cohead_shipto_cntct_first_name || ' ' || cohead_shipto_cntct_last_name 
                       FROM cohead
                       WHERE cohead_number = TRIM(SUBSTRING(lshist_ordernumber FROM '-.*-'), '-')
               ) ELSE '' END AS contact,  

    CASE WHEN (lshist_transtype = 'SH' AND 
                SUBSTRING(lshist_ordernumber FROM 1 FOR 2) = 'SO')
          THEN (SELECT cohead_shipto_cntct_phone
                       FROM cohead
                       WHERE cohead_number = TRIM(SUBSTRING(lshist_ordernumber FROM '-.*-'), '-')
               ) ELSE '' END AS contact_phone,                

    CASE WHEN (lshist_transtype = 'SH' AND 
                SUBSTRING(lshist_ordernumber FROM 1 FOR 2) = 'SO')
          THEN (SELECT cohead_shipto_cntct_fax
                       FROM cohead
                       WHERE cohead_number = TRIM(SUBSTRING(lshist_ordernumber FROM '-.*-'), '-')
               ) ELSE '' END AS contact_fax, 

    CASE WHEN (lshist_transtype = 'SH' AND 
                SUBSTRING(lshist_ordernumber FROM 1 FOR 2) = 'SO')
          THEN (SELECT cohead_shipto_cntct_email
                       FROM cohead
                       WHERE cohead_number = TRIM(SUBSTRING(lshist_ordernumber FROM '-.*-'), '-')
               ) ELSE '' END AS contact_email,                 

    CASE WHEN (lshist_transtype = 'SH' AND 
                SUBSTRING(lshist_ordernumber FROM 1 FOR 2) = 'SO')
          THEN (SELECT cohead_shiptophone FROM cohead
                       WHERE cohead_number = TRIM(SUBSTRING(lshist_ordernumber FROM '-.*-'), '-')
               ) ELSE '' END AS phone   
-- HMRF END


FROM lshist(&lt;? value("itemid") ?>,&lt;? value("warehouseid") ?>,&lt;? value("lotSerial") ?>,
&lt;? if exists("pattern") ?>
TRUE
&lt;? else ?>
FALSE
&lt;? endif ?>,
&lt;? value("transType") ?>,&lt;? value("startDate") ?>,&lt;? value("endDate") ?>,&lt;? value("trace") ?>,1), invhist, itemsite, item
WHERE ((invhist_id=lshist_id)
AND (invhist_itemsite_id=itemsite_id)
AND (itemsite_item_id=item_id));</sql>
 </querysource>
