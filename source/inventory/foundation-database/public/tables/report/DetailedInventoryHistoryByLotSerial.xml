 <title>Detailed Inventory History by Lot/Serial #</title>
 <name>DetailedInventoryHistoryByLotSerial</name>
 <description>HMRF - 4.9.3 - add ShipTo info for shipements</description>
 <parameter default="01/01/2007" active="true" listtype="static" type="string" name="endDate">
  <description></description>
 </parameter>
 <parameter default=".*" active="true" listtype="static" type="string" name="lotSerial">
  <description></description>
 </parameter>
 <parameter default="01/01/2006" active="true" listtype="static" type="string" name="startDate">
  <description></description>
 </parameter>
 <parameter default="Issues" active="true" listtype="static" type="string" name="transType">
  <description></description>
 </parameter>
 <grid>
  <snap/>
  <show/>
  <x>0.05</x>
  <y>0.05</y>
 </grid>
