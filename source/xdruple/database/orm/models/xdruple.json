          "isNaturalKey": true
        }
      },
      {
        "name": "site",
        "toOne": {
          "type": "XdrupleSite",
          "column": "xd_user_contact_site_id",
          "required": true
        }
      },
      {
        "name": "uid",
        "attr": {
          "type": "Number",
          "column": "xd_user_contact_uid"
