 <querysource>
  <name>Detail</name>
  <sql>SELECT brdvar_id,
       formatDate(brdvar_postdate) AS f_postdate,
       item_number,
       formatQtyPer(brdvar_stdqtyper) AS f_stdqtyper,
       formatQty(brdvar_stdqtyper * brdvar_wo_qty) AS f_stdqty,
       formatQtyPer(brdvar_actqtyper) AS f_actqtyper,
       formatQty(brdvar_actqtyper * brdvar_wo_qty) AS f_actqty,
       formatQtyPer(brdvar_actqtyper - brdvar_stdqtyper) AS f_qtypervar,
       formatPrcnt((brdvar_actqtyper - brdvar_stdqtyper) / brdvar_stdqtyper) AS f_pervar
  FROM xtmfg.brdvar, itemsite AS ps, itemsite AS cs, item
 WHERE ( (brdvar_parent_itemsite_id=ps.itemsite_id)
   AND (brdvar_itemsite_id=cs.itemsite_id)
   AND (cs.itemsite_item_id=item_id)
   AND (ps.itemsite_item_id=&lt;? value(&quot;item_id&quot;) ?>)
   AND (brdvar_postdate BETWEEN &lt;? value(&quot;startDate&quot;) ?> AND &lt;? value(&quot;endDate&quot;) ?>)
&lt;? if exists(&quot;warehous_id&quot;) ?>
   AND (ps.itemsite_warehous_id=&lt;? value(&quot;warehous_id&quot;) ?>)
&lt;? endif ?>
)
ORDER BY brdvar_postdate, item_number;</sql>
 </querysource>
 <querysource>
  <name>head</name>
  <sql>SELECT item_number,
       item_descrip1,
       item_descrip2,
       &lt;? if exists(&quot;warehous_id&quot;) ?>
         (SELECT warehous_code
            FROM whsinfo
           WHERE (warehous_id=&lt;? value(&quot;warehous_id&quot;) ?>))
       &lt;? else ?>
         text('All Warehouses')
       &lt;? endif ?>
       AS warehouse,
       formatDate(&lt;? value(&quot;startDate&quot;) ?>, 'Earliest') AS startdate,
       formatDate(&lt;? value(&quot;endDate&quot;) ?>, 'Latest') AS enddate
  FROM item
 WHERE (item_id=&lt;? value(&quot;item_id&quot;) ?>);</sql>
 </querysource>
