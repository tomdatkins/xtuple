 <querysource>
  <name>detail</name>
  <sql>SELECT stdopn_number,
       stdopn_descrip1, stdopn_descrip2,
       CASE WHEN (stdopn_wrkcnt_id &lt; 1)  THEN 'Any'
            ELSE (select wrkcnt_code
                  from xtmfg.wrkcnt
                  where wrkcnt_id=stdopn_wrkcnt_id)
       END AS wrkcnt,
       stdopn_toolref,
       stdopn_produom,
       formatRatio(stdopn_invproduomratio) AS f_invproduomratio,
       formatBoolYN(stdopn_stdtimes) AS f_stdtimes,
       CASE WHEN (stdopn_stdtimes=false) THEN '-'
            ELSE formatQty(stdopn_sutime)
       END AS f_sutime,
       CASE WHEN (stdopn_stdtimes=false) THEN '-'
            ELSE formatQty(stdopn_rntime)
       END AS f_rntime,
       CASE WHEN (stdopn_stdtimes=false) THEN '-'
            ELSE formatQty(stdopn_rnqtyper)
       END AS f_rnqtyper,
       CASE WHEN (stdopn_stdtimes=false) THEN '-'
            WHEN (stdopn_sucosttype='D') THEN 'Direct Labor'
            WHEN (stdopn_sucosttype='O') THEN 'Overhead'
            WHEN (stdopn_sucosttype='N') THEN 'None'
            ELSE stdopn_sucosttype
       END AS f_sucosttype,
       CASE WHEN (stdopn_stdtimes=false) THEN '-'
            WHEN (stdopn_rncosttype='D') THEN 'Direct Labor'
            WHEN (stdopn_rncosttype='O') THEN 'Overhead'
            WHEN (stdopn_rncosttype='N') THEN 'None'
            ELSE stdopn_rncosttype
       END AS f_rncosttype,
       CASE WHEN (stdopn_stdtimes=false) THEN '-'
            ELSE formatBoolYN(stdopn_reportrun)
       END AS f_sureport,
       CASE WHEN (stdopn_stdtimes=false) THEN '-'
            ELSE formatBoolYN(stdopn_reportrun)
       END AS f_rnreport,
       stdopn_instructions
  FROM xtmfg.stdopn
ORDER BY stdopn_number;</sql>
