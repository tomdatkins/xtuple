 <querysource>
  <name>head</name>
  <sql>SELECT formatWONumber(wo_id) AS wonumber,
       warehous_code as warehouse,
       item_number, item_descrip1,
       item_descrip2, uom_name AS iteminvuom,
       wo_status AS status
  FROM wo, itemsite, item, whsinfo, uom
 WHERE ((wo_itemsite_id=itemsite_id)
   AND (itemsite_item_id=item_id)
   AND (item_inv_uom_id=uom_id)
   AND (itemsite_warehous_id=warehous_id)
   AND (wo_id=&lt;? value("wo_id") ?>) );</sql>
 </querysource>
 <querysource>
  <name>detail</name>
  <sql>SELECT wooper_seqnumber,
       COALESCE(wrkcnt_code,    'Any') AS wrkcnt_code,
       COALESCE(wrkcnt_descrip, ''   ) AS wrkcnt_descrip,
       COALESCE(stdopn_number,  ''   ) AS stdoperation,
       wooper_descrip1, wooper_descrip2,
       CASE WHEN (wooper_sucomplete) THEN text('Complete')
            ELSE formatTime(noNeg(wooper_sutime - wooper_suconsumed))
       END AS setupremain,
       CASE WHEN (wooper_rncomplete) THEN text('Complete')
            ELSE formatTime(noNeg(wooper_rntime - wooper_rnconsumed))
       END AS runremain,
       formatQty(wooper_qtyrcv) AS f_qtyrcv
  FROM xtmfg.wooper
       LEFT OUTER JOIN xtmfg.wrkcnt ON (wrkcnt_id=wooper_wrkcnt_id)
       LEFT OUTER JOIN xtmfg.stdopn ON (stdopn_id=wooper_stdopn_id)
 WHERE (wooper_wo_id=&lt;? value("wo_id") ?>) 
ORDER BY wooper_seqnumber;</sql>
 </querysource>
