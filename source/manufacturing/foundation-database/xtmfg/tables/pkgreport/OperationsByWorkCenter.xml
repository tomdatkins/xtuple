 <querysource>
  <name>head</name>
  <sql>SELECT wrkcnt_code,
       wrkcnt_descrip,
       warehous_code
  FROM xtmfg.wrkcnt, whsinfo
 WHERE ((wrkcnt_warehous_id=warehous_id)
   AND (wrkcnt_id=&lt;? value("wrkcnt_id") ?>));
    </sql>
 </querysource>
 <querysource>
  <name>detail</name>
  <sql>SELECT item_number, item_descrip1, item_descrip2,
       booitem_seqnumber, booitem_execday,
       CASE WHEN (booitem_stdopn_id=-1) THEN 'None'
            ELSE ( SELECT stdopn_number
                   FROM xtmfg.stdopn
                   WHERE (stdopn_id=booitem_stdopn_id) )
       END AS stdoperation,
       (booitem_descrip1 || ' ' || booitem_descrip2) AS operdescrip,
       booitem_toolref, booitem_instruc,
       formatTime(booitem_sutime) AS setuptime,
       formatTime(booitem_rntime) AS runtime,
       formatDate(booitem_effective, 'Always') AS effective,
       formatDate(booitem_expires, 'Never') AS expires
  FROM xtmfg.booitem, item
 WHERE ((booitem_item_id=item_id)
   AND (booitem_wrkcnt_id=&lt;? value("wrkcnt_id") ?>))
ORDER BY item_number, booitem_seqnumber;

       </sql>
 </querysource>
