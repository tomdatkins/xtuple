 <querysource>
  <name>detail</name>
  <sql>SELECT wrkcnt_code, wrkcnt_descrip, dept_number, warehous_code,
               wrkcnt_nummachs, wrkcnt_numpeople,
               COALESCE(r.lbrrate_code || ' (' || formatExtPrice(r.lbrrate_rate) || ')',
                        formatExtPrice(wrkcnt_runrate)) AS runrate,
               COALESCE(s.lbrrate_code || ' (' || formatExtPrice(s.lbrrate_rate) || ')',
                        formatExtPrice(wrkcnt_setuprate)) AS setuprate,
                formatScrap(wrkcnt_brd_prcntlbr) AS prcntlbr,
                formatExtPrice(wrkcnt_brd_rateperlbrhr) AS rateperlbrhr,
                formatExtPrice(wrkcnt_brd_ratepermachhr) AS ratepermachhr,
                formatCost((wrkcnt_runrate * wrkcnt_brd_prcntlbr) +
                            wrkcnt_brd_rateperlbrhr + wrkcnt_brd_ratepermachhr) AS ratehr,
                formatCost(wrkcnt_brd_rateperunitprod) AS rateperunitprod,
                formatExtPrice(wrkcnt_avgqueuedays) AS avgqueuedays,
                formatTime(wrkcnt_avgsutime) AS avgsutime,
                formatCost(wrkcnt_dailycap) AS dailycap,
                formatCost(wrkcnt_efficfactor) AS efffactor,
                CASE WHEN(wrkcnt_caploaduom='L') THEN 'Labor Time'
                     WHEN(wrkcnt_caploaduom='M') THEN 'Machine Time'
                     ELSE wrkcnt_caploaduom
                END AS caploaduom,
                wrkcnt_comments
   FROM whsinfo, xtmfg.wrkcnt
     LEFT OUTER JOIN dept ON (wrkcnt_dept_id=dept_id)
     LEFT OUTER JOIN xtmfg.lbrrate AS r ON (r.lbrrate_id=wrkcnt_run_lbrrate_id)
     LEFT OUTER JOIN xtmfg.lbrrate AS s ON (s.lbrrate_id=wrkcnt_setup_lbrrate_id)
WHERE (wrkcnt_warehous_id=warehous_id)
ORDER BY wrkcnt_code;</sql>
