SELECT booitem_seq_id,
       booitem_seqnumber,
       booitem_descrip1,
       booitem_descrip2
  FROM xtmfg.booitem(<? value("item_id") ?>) 
 WHERE((CURRENT_DATE BETWEEN booitem_effective AND booitem_expires)) 
 ORDER BY booitem_seqnumber;

