SELECT stdopn_id, stdopn_number, stdopn_active,
       (COALESCE(stdopn_descrip1, '') || ' ' || COALESCE(stdopn_descrip2, '')) AS descrip
  FROM xtmfg.stdopn
 ORDER BY stdopn_number;
