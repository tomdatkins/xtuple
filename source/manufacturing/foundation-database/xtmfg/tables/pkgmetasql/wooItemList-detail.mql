SELECT wooper_seqnumber as wooper_seq_id,
wooper_seqnumber,
wooper_descrip1,
wooper_descrip2
FROM xtmfg.wooper
WHERE (wooper_wo_id = <? value("item_id") ?>) 
ORDER BY wooper_seqnumber;
