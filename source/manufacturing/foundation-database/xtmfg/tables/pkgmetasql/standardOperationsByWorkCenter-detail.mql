SELECT stdopn_id, stdopn_number,
       (stdopn_descrip1 || ' ' || stdopn_descrip2) AS stdopndescrip
  FROM xtmfg.stdopn
 WHERE (stdopn_wrkcnt_id=<? value("wrkcnt_id") ?>)
 ORDER BY stdopn_number;

