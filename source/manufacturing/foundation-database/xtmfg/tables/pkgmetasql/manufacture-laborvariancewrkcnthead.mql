SELECT
<? if exists("wrkcnt_id") ?>
       wrkcnt_code,
       wrkcnt_descrip,
<? else ?>
       <? value("all_workcenters") ?> AS wrkcnt_code,
       '' AS wrkcnt_descrip,
<? endif ?>
       formatDate(<? value("startDate") ?>, 'Earliest') AS startdate,
       formatDate(<? value("endDate") ?>, 'Latest') AS enddate
   FROM xtmfg.wrkcnt
<? if exists("wrkcnt_id") ?>
   WHERE (wrkcnt_id=<? value("wrkcnt_id") ?>)
<? endif ?>
;
